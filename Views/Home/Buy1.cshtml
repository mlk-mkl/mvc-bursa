@{
    ViewData["Title"] = "Список товаров";
}
<h2>@ViewData["Title"]</h2>

<p><a asp-page="Create">Добавить товар</a></p>
<table class="table">
    <thead>
        <tr><th>Название</th><th>Цена</th></tr>
    </thead>
    <tbody>
        @foreach (var product in ViewBag.)
        {
            <tr><td>@product.Name</td><td>@product.Price</td></tr>
        }
    </tbody>
</table>